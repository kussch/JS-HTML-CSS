<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
  <p>One</p>
  <p class='elem'>Two</p>
  <p class='elem'>Three</p>
<script type="text/javascript" src='script.js'></script>
</body>
</html

-----------------------------------------------------

let p = document.querySelectorAll('.elem');

for (let i = 0; i < p.length; i++) {

	p[i].onclick = function(){
		alert('you click on some DOM-tag');
	}
}

console.log(p);

